<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eus.ehu.cinemaProject.ui.ReceiptController">
   <children>
      <Button fx:id="goBack" layoutX="63.0" layoutY="322.0" mnemonicParsing="false" onAction="#goToSeatSelect" prefHeight="26.0" prefWidth="46.0" text="Back" />
      <Button fx:id="proceedPayment" layoutX="430.0" layoutY="322.0" mnemonicParsing="false" onAction="#proceedPaymentButton" text="Proceed with payment" />
      <Button fx:id="orderSnacks" layoutX="254.0" layoutY="321.0" mnemonicParsing="false" onAction="#goToSnacksSelect" prefHeight="26.0" prefWidth="91.0" text="Order Snacks" />
      <TextField fx:id="movieInfo" layoutX="64.0" layoutY="68.0" prefHeight="202.0" prefWidth="254.0" promptText="Information about the movie" />
      <TextField fx:id="receipt" layoutX="391.0" layoutY="75.0" prefHeight="170.0" prefWidth="182.0" promptText="Actual receipt (Price of each seat and price of snacks)" />
      <TextField fx:id="totalPrize" layoutX="457.0" layoutY="245.0" prefHeight="26.0" prefWidth="116.0" />
      <Label layoutX="402.0" layoutY="250.0" text="TOTAL:" />
   </children>
</AnchorPane>
